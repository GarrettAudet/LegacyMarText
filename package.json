{
  "name": "martext",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "build": "npm run build --prefix ./client",
    "build-and-start": "npm run build&&npm start",
    "dev": "nodemon ./app.js",
    "dev-both": "concurrently \"npm run dev\" \"npm start --prefix ./client\""
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "concurrently": "^4.1.1"
  },
  "dependencies": {
    "fuzzy-search": "^3.0.1",
    "@cloudant/cloudant": "^4.1.1",
    "@google/maps": "^0.5.5",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "ibm-watson": "^4.2.1",
    "twilio": "^3.33.0"
  }
}
